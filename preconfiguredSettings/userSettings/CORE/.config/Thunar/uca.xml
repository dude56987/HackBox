	<name>Open Terminal Here</name>
	<unique-id>1485302619370690-3</unique-id>
	<command>guake --show --new-tab %f</command>
	<description>Example for a custom action</description>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>printer-printing-symbolic</icon>
	<name>Print File</name>
	<unique-id>1485302692406837-5</unique-id>
	<command>lp %f</command>
	<description>Prints the Selected file</description>
	<patterns>*</patterns>
	<startup-notify/>
	<image-files/>
	<text-files/>
</action>
<action>
	<icon>media-playback-start-symbolic</icon>
	<name>Play selected videos at random</name>
	<unique-id>1485302736234101-6</unique-id>
	<command>vlc %F -Z</command>
	<description>Plays selected videos randomly</description>
	<patterns>*</patterns>
	<startup-notify/>
	<video-files/>
</action>
<action>
	<icon>audio-x-generic-symbolic</icon>
	<name>Convert to audio</name>
	<unique-id>1485302668964731-4</unique-id>
	<command>avconv -i %f %f.mp3</command>
	<description>Converts selected video to mp3 file</description>
	<patterns>*</patterns>
	<video-files/>
</action>
<action>
	<icon>emblem-system-symbolic</icon>
	<name>Compress PDF</name>
	<unique-id>1485303128648566-7</unique-id>
	<command>terminator -x gs -sDEVICE=pdfwrite -dCompatibilityLevel=1.4 -dPDFSETTINGS=/screen -dNOPAUSE -dQUIET -dBATCH -sOutputFile=%d/COMPRESSED_%n %f</command>
	<description>Compresses a pdf file</description>
	<patterns>*.pdf</patterns>
	<startup-notify/>
	<other-files/>
</action>
<action>
	<icon>application-x-appliance-symbolic</icon>
	<name>Crop PDF</name>
	<unique-id>1485303150190173-8</unique-id>
	<command>terminator -x pdfcrop %f %d/CROPED_%n</command>
	<description>Crops the edges off a pdf</description>
	<patterns>*.pdf</patterns>
	<startup-notify/>
	<other-files/>
</action>
