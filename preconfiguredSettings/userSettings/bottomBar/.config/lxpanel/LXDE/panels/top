Global {
    edge=right
    allign=center
    margin=0
    widthtype=request
    width=549
    height=100
    transparent=0
    tintcolor=#ffffff
    alpha=255
    autohide=1
    heightwhenhidden=2
    setdocktype=1
    setpartialstrut=0
    usefontcolor=0
    fontsize=10
    fontcolor=#000000
    usefontsize=0
    background=0
    iconsize=24
    loglevel=2
}

Plugin {
    type = cpu
}

