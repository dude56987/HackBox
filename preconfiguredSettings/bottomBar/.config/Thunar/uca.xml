<?xml encoding="UTF-8" version="1.0"?>
<actions>
<action>
	<icon>terminator</icon>
	<name>Open Terminal Here</name>
	<unique-id>1392065039449149-4</unique-id>
	<command>terminator --working-directory=%f</command>
	<description>Example for a custom action</description>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>fileprint</icon>
	<name>Print File</name>
	<unique-id>1392064917689115-2</unique-id>
	<command>lp %f</command>
	<description>Prints the Selected file</description>
	<patterns>*</patterns>
	<startup-notify/>
	<image-files/>
	<text-files/>
</action>
<action>
	<icon>vlc</icon>
	<name>Play selected videos at random</name>
	<unique-id>1388707902607947-3</unique-id>
	<command>vlc %F -Z</command>
	<description>Plays selected videos randomly</description>
	<patterns>*</patterns>
	<startup-notify/>
	<video-files/>
</action>
<action>
	<icon>vlc</icon>
	<name>Play All Videos in folder randomly</name>
	<unique-id>1388707902607969-4</unique-id>
	<command>vlc -Z %d/*.*</command>
	<description>Randomly plays all videos in current folder</description>
	<patterns>*.*</patterns>
	<startup-notify/>
	<video-files/>
</action>
<action>
	<icon>soundconverter</icon>
	<name>Convert to audio</name>
	<unique-id>1388707902607988-5</unique-id>
	<command>ffmpeg -i %f %f.mp3</command>
	<description>Converts selected video to mp3 file</description>
	<patterns>*</patterns>
	<video-files/>
</action>
<action>
	<icon>application-pdf</icon>
	<name>Compress PDF</name>
	<unique-id>1388707902608009-6</unique-id>
	<command>terminator -x gs -sDEVICE=pdfwrite -dCompatibilityLevel=1.4 -dPDFSETTINGS=/screen -dNOPAUSE -dQUIET -dBATCH -sOutputFile=%d/COMPRESSED_%n %f</command>
	<description>Compresses a pdf file</description>
	<patterns>*.pdf</patterns>
	<startup-notify/>
	<other-files/>
</action>
<action>
	<icon>application-pdf</icon>
	<name>Crop PDF</name>
	<unique-id>1388707902608030-7</unique-id>
	<command>terminator -x pdfcrop %f %d/CROPED_%n</command>
	<description>Crops the edges off a pdf</description>
	<patterns>*.pdf</patterns>
	<startup-notify/>
	<other-files/>
</action>
<action>
	<icon>gimp</icon>
	<name>Edit with GIMP</name>
	<unique-id>1388707902608049-8</unique-id>
	<command>gimp %F</command>
	<description>Edit the selected file with gimp</description>
	<patterns>*</patterns>
	<startup-notify/>
	<image-files/>
</action>
<action>
	<icon>audacity</icon>
	<name>Edit with Audacity</name>
	<unique-id>1388707902608068-9</unique-id>
	<command>audacity %F</command>
	<description>Edit sound file with Audacity</description>
	<patterns>*</patterns>
	<startup-notify/>
	<audio-files/>
</action>
<action>
	<icon>java</icon>
	<name>Run Java Program</name>
	<unique-id>1388707902608088-10</unique-id>
	<command>java -jar %f</command>
	<description>Execute .jar files</description>
	<patterns>*.jar</patterns>
	<startup-notify/>
	<other-files/>
</action>
<action>
	<icon>mcomix</icon>
	<name>Read Comic</name>
	<unique-id>1388707902608108-11</unique-id>
	<command>mcomix %F</command>
	<description>Opens a compressed comic archive</description>
	<patterns>*.zip;*.rar</patterns>
	<startup-notify/>
	<other-files/>
</action>
<action>
	<icon>bash</icon>
	<name>Run Shell Script</name>
	<unique-id>1388707902608129-12</unique-id>
	<command>terminator -x bash %f</command>
	<description>Run a bash shell script</description>
	<patterns>*.sh</patterns>
	<startup-notify/>
	<other-files/>
	<text-files/>
</action>
<action>
	<icon>python</icon>
	<name>Run Python Script</name>
	<unique-id>1388707902608150-13</unique-id>
	<command>terminator -x python %f</command>
	<description>Runs a python script with python interpeter</description>
	<patterns>*.py;*.pyc</patterns>
	<startup-notify/>
	<other-files/>
	<text-files/>
</action>
<action>
	<icon>emblem-shared-symbolic</icon>
	<name>Share file on LAN</name>
	<unique-id>1389234779365916-1</unique-id>
	<command>terminator -x bash -c &quot;echo &apos;Starting share, Press Control+C to stop the share...&apos;;echo %f;echo &apos;Connect to &apos;$(hostname)&apos;.local:9119 to download.&apos;;woof -p 9119 $(echo %f | sed &apos;s/ /\\ /g&apos;);echo &apos;Share is now complete, press enter to close the window...&apos;;read;&quot;#NOTES: %f is in single quotes, you cant store bash variables this way</command>
	<description>Share file  on the LAN once then close</description>
	<patterns>*</patterns>
	<startup-notify/>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
<action>
	<icon>emblem-shared-symbolic</icon>
	<name>Share directory on LAN</name>
	<unique-id>1388707902608212-15</unique-id>
	<command>terminator -x bash -c &quot;echo &apos;Starting share, Press Control+C to stop the share...&apos;;echo %f;echo &apos;Connect to &apos;$(hostname)&apos;.local:9119 to download.&apos;;woof -p 9119 $(echo %f | sed &apos;s/ /\\ /g&apos;);echo &apos;Share is now complete, press enter to close the window...&apos;;read;&quot;#NOTES: %f is in single quotes, you cant store bash variables this way</command>
	<description>Share directory on LAN once then closes</description>
	<patterns>*</patterns>
	<startup-notify/>
	<directories/>
</action>
<action>
	<icon>system-run</icon>
	<name>Run Make</name>
	<unique-id>1393924088471499-8</unique-id>
	<command>terminator -x bash -c &quot;make -f %f;echo &apos;Press enter to close this window...&apos;;read;&quot;</command>
	<description>Run Make</description>
	<patterns>makefile</patterns>
	<startup-notify/>
	<text-files/>
</action>
<action>
	<icon>search</icon>
	<name>Search</name>
